#!/bin/ruby

require 'json'
require 'stringio'

#
# Complete the 'caesarCipher' function below.
#
# The function is expected to return a STRING.
# The function accepts following parameters:
#  1. STRING s
#  2. INTEGER k
#

def caesarCipher(s, k)
    # Write your code here
     message = s.split('')
    position_array = message.map {|letter| letter.bytes }
    p position_array
    position_array.each {|element| element[0] += k}
    p position_array
    position_array.each {|element| element.pack('C*').force_encoding('UTF-8')}
    p position_array

end

fptr = File.open(ENV['OUTPUT_PATH'], 'w')

n = gets.strip.to_i

s = gets.chomp

k = gets.strip.to_i

result = caesarCipher s, k

fptr.write result
fptr.write "\n"

fptr.close()
